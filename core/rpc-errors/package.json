{
    "name": "@canton-network/core-rpc-errors",
    "version": "0.13.3",
    "type": "module",
    "description": "Wrapper for JSON-RPC error objects",
    "author": "Alex Matson <alex.matson@digitalasset.com>",
    "packageManager": "yarn@4.9.4",
    "license": "Apache-2.0",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/**"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup && tsc -p tsconfig.types.json",
        "dev": "tsup --watch --onSuccess \"tsc -p tsconfig.types.json\"",
        "flatpack": "yarn pack --out \"$FLATPACK_OUTDIR\"",
        "clean": "tsc -b --clean; rm -rf dist"
    },
    "dependencies": {
        "@metamask/rpc-errors": "^7.0.3"
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hyperledger-labs/splice-wallet-kernel.git",
        "directory": "core/rpc-errors"
    }
}
