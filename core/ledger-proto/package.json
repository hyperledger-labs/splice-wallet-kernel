{
    "name": "@canton-network/core-ledger-proto",
    "version": "0.10.0",
    "type": "module",
    "description": "Provides TypeScript protobuf bindings for Canton",
    "repository": "github:hyperledger-labs/splice-wallet-kernel",
    "license": "Apache-2.0",
    "author": "Phillip Olesen <phillip.olesen@digitalasset.com>",
    "packageManager": "yarn@4.9.4",
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.cjs",
            "default": "./dist/esm/index.js"
        }
    },
    "scripts": {
        "build": "tsup && tsc -b tsconfig.types.json",
        "dev": "tsup --watch --onSuccess \"tsc -b tsconfig.types.json\"",
        "flatpack": "yarn pack --out \"$FLATPACK_OUTDIR\"",
        "clean": "tsc -b --clean; rm -rf dist"
    },
    "devDependencies": {
        "@swc/core": "^1.11.31",
        "@types/node": "^22.15.30",
        "esbuild-plugin-replace": "^1.4.0",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@protobuf-ts/runtime": "^2.11.1",
        "@protobuf-ts/runtime-rpc": "^2.11.1"
    },
    "files": [
        "dist/**"
    ],
    "publishConfig": {
        "access": "public"
    }
}
