{
    "name": "@canton-network/core-wallet-user-rpc-client",
    "version": "0.23.0",
    "type": "module",
    "description": "TypeScript client generated by OpenRPC",
    "license": "Apache-2.0",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup && tsc -p tsconfig.types.json && yarn run docs",
        "docs": "typedoc --tsconfig tsconfig.docs.json",
        "dev": "tsup --watch --onSuccess \"tsc -p tsconfig.types.json && yarn run docs\"",
        "flatpack": "yarn pack --out \"$FLATPACK_OUTDIR\"",
        "clean": "tsc -b --clean; rm -rf dist"
    },
    "files": [
        "dist/**"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@canton-network/core-rpc-transport": "workspace:^",
        "@canton-network/core-types": "workspace:^",
        "lodash": "^4.17.23"
    },
    "devDependencies": {
        "@types/isomorphic-fetch": "^0.0.39",
        "@types/jest": "^30.0.0",
        "@types/json-schema": "7.0.15",
        "@types/lodash": "^4.17.23",
        "@types/ws": "^8.18.1",
        "globals": "^16.5.0",
        "prettier": "^3.8.1",
        "tsup": "^8.5.1",
        "typedoc": "^0.28.16",
        "typescript": "^5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hyperledger-labs/splice-wallet-kernel.git",
        "directory": "core/wallet-user-rpc-client"
    }
}
